{
  "name": "prompt-manager",
  "version": "1.0.0",
  "description": "A prompt management library for AI applications",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "prompt-manager": "src/cli/cli.ts"
  },
  "scripts": {
    "generate": "bun run scripts/generate.ts",
    "build": "tsup",
    "prepublishOnly": "tsup",
    "ex-prepare": "bun run build",
    "test": "bun test",
    "start": "bun run src/index.ts",
    "test:coverage": "bun test --coverage",
    "tsc": "tsc",
    "tsc:fix": "tsc --noEmit --fix",
    "prompt-manager": "bun run src/cli/cli.ts",
    "publish": "npm publish",
    "legacy-build": "bun run generate && bun run rollup -c",
    "legacy-prepublishOnly": "bun run build",
    "docs:build": "typedoc --out docs src",
    "build:all": "npm run build && npm run docs:build",
    "dev": "tsup --watch",
    "install-global": "npm install -g .",
    "dev:watch": "concurrently \"bun run dev\" \"bun run install-global\" \"bun run install-example\"",
    "install-example": "cd examples/test-project && npm install",
    "lint": "oxlint . --ext .ts",
    "lint:fix": "oxlint --quiet --fix .",
    "test:cleanup": "bun run scripts/cleanupTests.ts"
  },
  "keywords": [
    "prompt",
    "management",
    "AI"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.33",
    "@ai-sdk/openai": "^0.0.40",
    "@inquirer/prompts": "^5.3.2",
    "@types/inquirer": "^9.0.7",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "@types/json-schema": "^7.0.15",
    "@types/text-encoding": "^0.0.39",
    "ai": "^3.2.37",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "console-table-printer": "^2.12.1",
    "cosmiconfig": "^9.0.0",
    "debug": "^4.3.6",
    "fs-extra": "^11.2.0",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "proper-lockfile": "^4.1.2",
    "text-encoding": "^0.7.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.12",
    "@types/proper-lockfile": "^4.1.4",
    "concurrently": "^8.2.2",
    "oxlint": "^0.6.1",
    "pre-commit": "^1.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.3",
    "typescript": ">=4.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  }
}
