{
  "name": "prompt-manager",
  "version": "1.0.0",
  "description": "A prompt management library for AI applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run generate && rollup -c",
    "generate": "ts-node scripts/generate.ts",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "start": "ts-node src/index.ts",
    "test:coverage": "jest --coverage",
    "prompt-manager": "ts-node scripts/cli.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  },
  "bin": {
    "prompt-manager": "./dist/cli.js"
  },
  "keywords": [
    "prompt",
    "management",
    "AI"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "jest": "^29.7.0",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  }
}
