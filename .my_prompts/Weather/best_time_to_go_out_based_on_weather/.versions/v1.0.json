{
  "name": "best_time_to_go_out_based_on_weather",
  "category": "Weather",
  "description": "Determine the best time to go out based on current and forecasted weather conditions.",
  "version": "1.0",
  "template": "Given the current weather conditions and the forecast for the day, suggest the best time to go out for outdoor activities. Consider factors such as temperature, precipitation, wind speed, and humidity.",
  "parameters": [
    "currentWeather",
    "forecast",
    "activityType"
  ],
  "metadata": {
    "created": "2023-10-01",
    "lastModified": "2023-10-01"
  },
  "outputType": "structured",
  "defaultModelName": "weather_model",
  "inputSchema": {
    "type": "object",
    "properties": {
      "currentWeather": {
        "type": "object",
        "properties": {
          "temperature": {
            "type": "number"
          },
          "precipitation": {
            "type": "string"
          },
          "windSpeed": {
            "type": "number"
          },
          "humidity": {
            "type": "number"
          }
        }
      },
      "forecast": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "time": {
              "type": "string"
            },
            "temperature": {
              "type": "number"
            },
            "precipitation": {
              "type": "string"
            },
            "windSpeed": {
              "type": "number"
            },
            "humidity": {
              "type": "number"
            }
          }
        }
      },
      "activityType": {
        "type": "string"
      }
    }
  },
  "outputSchema": {
    "type": "object",
    "properties": {
      "bestTime": {
        "type": "string"
      },
      "reasoning": {
        "type": "string"
      }
    }
  },
  "configuration": {
    "modelName": "weather_model",
    "temperature": 0.7,
    "maxTokens": 100,
    "topP": 1,
    "frequencyPenalty": 0,
    "presencePenalty": 0,
    "stopSequences": []
  }
}